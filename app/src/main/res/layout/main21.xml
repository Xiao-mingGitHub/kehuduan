<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorPrimaryDark"
    tools:context=".Main21">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        >
        <View
            android:layout_width="wrap_content"
            android:layout_height="28dp"
            android:background="@color/colorPrimaryDark"
            />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:id="@+id/main4_1"
            android:background="@color/colorPrimaryDark"
            >

            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:src="@drawable/bak"
                android:id="@+id/main21_bak"
                android:layout_centerVertical="true"

                />
            <TextView
                android:layout_toRightOf="@id/main21_bak"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="不动产服务"
                android:textSize="16sp"
                android:textColor="@color/colorText_search"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                />
            <ImageView
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:src="@drawable/b_6"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="7dp"
                />

            <TextView
                android:layout_width="78dp"
                android:layout_height="28dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:background="@drawable/backcc"
                android:id="@+id/backk"
                />
            <View
                android:layout_width="1px"
                android:layout_height="17dp"
                android:background="#CCCCCC"
                android:layout_alignParentRight="true"
                android:layout_marginRight="39dp"
                android:layout_centerVertical="true"
                />


            <ImageView
                android:layout_width="20dp"
                android:layout_height="12dp"
                android:src="@drawable/b_b"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="48dp"
                />
        </RelativeLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/tp51_01" />
        <ImageView
            android:id="@+id/main21g"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/tp51_02" />

        <ImageView
            android:id="@+id/main21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:src="@drawable/tp51_03" />




    </LinearLayout>
    <RelativeLayout
        android:id="@+id/main21_tishi"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#7A696969"
        android:visibility="gone"

        >
        <LinearLayout
            android:layout_marginRight="18dp"
            android:layout_marginLeft="18dp"
            android:layout_centerInParent="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/tishibak"
            android:orientation="vertical"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="提示"
                android:layout_gravity="center_horizontal"
                android:textSize="18sp"
                android:textColor="@color/colorText_search"
                android:layout_marginTop="25dp"
                android:textStyle="bold"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="本次操作需要您进行登录验证"
                android:layout_gravity="center_horizontal"
                android:textSize="17sp"
                android:textColor="@color/colorx102"
                android:layout_marginTop="15dp"
                />
            <TextView
                android:layout_marginTop="30dp"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/colorx150"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:id="@+id/main21_quexiao"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="58dp"
                    android:text="取消"
                    android:textSize="18sp"
                    android:textColor="@color/colorText_search"
                    android:gravity="center"
                    android:textStyle="bold"
                    />
                <TextView
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:background="@color/colorx150"
                    />
                <TextView
                    android:id="@+id/main21_queren"
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="58dp"
                    android:text="确定"
                    android:textSize="18sp"
                    android:textColor="#586b95"
                    android:gravity="center"
                    android:textStyle="bold"
                    />

            </LinearLayout>



        </LinearLayout>



    </RelativeLayout>




</RelativeLayout>